{
  "name": "ac",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:allure": "npx playwright test",
    "allure:generate": "allure generate ./allure-results --clean",
    "allure:open": "allure open ./allure-report",
    "allure:report": "npm run allure:generate && npm run allure:open",
    "allure:clear": "rm -rf allure-results allure-report || true",
    "allure:full": "npm run allure:clear && npm run test:allure && npm run allure:report",
    "allure:history": "mkdir -p allure-results/history && cp -r allure-report/history/* allure-results/history/ 2>/dev/null || echo \"No history found\""
  },
  "keywords": [],
  "author": "",
