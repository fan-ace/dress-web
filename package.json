  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/node": "^22.15.14",
    "@types/ssh2": "^1.15.5",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.2.1",
    "cross-env": "^7.0.3"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "fs": "^0.0.1-security",
    "mysql2": "^3.14.1",
    "pg": "^8.15.6",
    "ssh2": "^1.16.0"
  }
}
